{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "noImplicitAny": false,
    "noEmit": true,
    "lib": [
      "esnext",
      "dom"
    ],
    "esModuleInterop": true,
    "typeRoots": [
      "typings",
      "node_modules/@types/"
    ]
  },
  "exclude": [
    "node_modules",
    "node_modules/@types/moment-timezone/moment-timezone.d.ts",
    "node_modules/moment-timezone/index.d.ts"
  ]

}
