{
  "name": "llama",
  "version": "0.1.0",
  "description": "Day Trading Bot",
  "main": "index.ts",
  "scripts": {
    "historian": "NODE_ENV=antsle LOG_LEVEL=info ts-node index.ts -f historian",
    "quant": "NODE_ENV=antsle LOG_LEVEL=info ts-node index.ts -f quant",
    "recorder": "NODE_ENV=antsle LOG_LEVEL=info ts-node index.ts -f recorder",
    "processor": "NODE_ENV=antsle LOG_LEVEL=info ts-node index.ts -f processor",
    "devHistorian_silly": "NODE_ENV=localAntsle LOG_LEVEL=silly ts-node index.ts -f historian",
    "devHistorian": "NODE_ENV=localAntsle ts-node index.ts -f historian",
    "devQuant": "NODE_ENV=localAntsle LOG_LEVEL=info ts-node index.ts -f quant",
    "devBroker": "NODE_ENV=localAntsle LOG_LEVEL=silly ts-node index.ts -f broker",
    "devHistorianLocal": "NODE_ENV=localDev LOG_LEVEL=silly ts-node index.ts -f historian",
    "devCryptoRecorderLocal": "NODE_ENV=localAntsle LOG_LEVEL=debug ts-node index.ts -f cryptoRecorder",
    "devRecorderLocal": "NODE_ENV=localDev LOG_LEVEL=debug ts-node index.ts index.ts -f recorder",
    "devProcessorLocal": "NODE_ENV=localDev LOG_LEVEL=debug ts-node index.ts -f processor",
    "devQuantLocal": "NODE_ENV=localDev LOG_LEVEL=debug ts-node index.ts -f quant",
    "inspectHistorianLocal": "NODE_ENV=localDev LOG_LEVEL=silly node --inspect=127.0.0.1:9230 -r ts-node/register index.ts -f historian",
    "inspectRecorderLocal": "NODE_ENV=localDev LOG_LEVEL=silly node --inspect -r ts-node/register index.ts -f recorder",
    "heroku_historian": "NODE_ENV=heroku_dev ts-node index.ts -f historian",
    "heroku_quant": "NODE_ENV=heroku_dev ts-node index.ts -f quant",
    "test": "NODE_ENV=localDev jest",
    "test_local": "NODE_ENV=localDev jest",
    "buildnPush": "docker build -f ./Llama.dockerfile . -t ctartist621/llama:latest; docker push ctartist621/llama:latest",
    "ptLogspout": "docker run --restart=always -d -v=/var/run/docker.sock:/var/run/docker.sock gliderlabs/logspout syslog+tls://logs6.papertrailapp.com:46794",
    "influxdb": "docker run --restart=always -d -v influxdb:/hdd/influxdb --name influxdb -P quay.io/influxdb/influxdb",
    "redis": "docker run --restart=always -d -v redis:/hdd/redis --name redis -P redis",
    "dockerHistorian": "docker run --restart=always --name Historian ctartist621/llama yarn historian"
  },
  "author": "David B Mittelman",
  "license": "UNLICENSED",
  "dependencies": {
    "@alpacahq/alpaca-trade-api": "^1.2.7",
    "@influxdata/influx": "^0.5.5",
    "@types/moment-timezone": "^0.5.12",
    "article-parser": "^4.1.1",
    "async": "^3.1.1",
    "commander": "^4.1.0",
    "config": "^3.2.4",
    "cron": "^1.8.2",
    "csv": "^5.3.1",
    "limiter": "^1.1.5",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.31",
    "needle": "^2.4.0",
    "redis": "^3.1.1",
    "retry": "^0.12.0",
    "sentiment": "^5.0.2",
    "talib": "^1.1.3",
    "tulind": "^0.8.18",
    "winston": "^3.2.1",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/async": "^3.0.5",
    "@types/jest": "24.0.19",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.5.0",
    "babel-jest": "^25.1.0",
    "heapdump": "^0.3.15",
    "jest": "^25.1.0",
    "jest-extended": "^0.11.4",
    "ts-jest": "^25.0.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  }
}
